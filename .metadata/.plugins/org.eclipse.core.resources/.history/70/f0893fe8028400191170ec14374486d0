package Objects;

public class testObjects {
	
	public static void main(String v[])
	{
		employee e1=new employee("Deepak","389524");
		employee e2=new employee("Vinithra","326112");
		employee e3=new employee("Nagesh","328988");
		employee e4=new employee("Vinithra","326112");

		
		System.out.println(e1);
		System.out.println(e2);
		System.out.println(e3);

		System.out.println(e1.hashCode());
		System.out.println(e2.hashCode());
		System.out.println(e3.hashCode());

		System.out.println("check equals : "+e2.equals(e4));
	}

}
