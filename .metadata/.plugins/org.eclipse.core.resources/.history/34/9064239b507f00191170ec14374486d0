package androidRevision;

import java.io.File;

import org.openqa.selenium.remote.DesiredCapabilities;

import io.appium.java_client.android.AndroidDriver;
import io.appium.java_client.remote.MobileCapabilityType;

public class firstProgram {

	public static void main(String[] args) {
		
		//File apk=new File("C:\\Users\\Deepak\\Selenium\\Java_selenium_May2nd\\Appium\\src\\ApiDemos-debug.apk");
		
		File f=new File("src");
		File fs=new File(f,"ApiDemos-debug.apk");
		DesiredCapabilities cap=new DesiredCapabilities();
		cap.setCapability(MobileCapabilityType.DEVICE_NAME, "Demo");
		cap.setCapability(MobileCapabilityType.APP, fs);
		
		AndroidDriver driver=new AndroidDriver(new URL(""), cap);
	}

}
