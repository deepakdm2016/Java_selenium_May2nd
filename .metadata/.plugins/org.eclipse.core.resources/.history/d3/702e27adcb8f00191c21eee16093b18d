package java8Bharath;

public class StaticDemoMethod {
	
	public static void main(String v[])
	{
		
		StaticDemoMethod.method1();
		System.out.println("Hello!: In main method");
	
	}
		static
		{
			StaticDemoMethod.method1();

			System.out.println("Hello!: Let's demonstrate static");
		}
	
	static void method1()
	{
		System.out.println("Inside method 1");
	}

}
