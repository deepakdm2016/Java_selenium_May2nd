package test;

import org.testng.annotations.DataProvider;
import org.testng.annotations.Test;

public class basicClass2 {
	
	@Test(priority=1,groups={"regression"})
	public void method1()
	{
		System.out.println("God is great Savior of families in baseclass2");
	}

	@Test(dataProvider="method3",priority=1,groups={"regression"})
	public void methodx()
	{
		System.out.println("method x - God is great Savior of families in baseclass2");
	}

	
	@Test(dataProvider="method3")
	public void method2(String x, String y, String z)
	{
		System.out.println("method 2 - God is great Savior of families in baseclass2");
		
		System.out.println(x);
		System.out.println(y);
		System.out.println(z);

		
	}

	@DataProvider
	public Object[][] method3()
	{
		
		Object[][] abc=new Object[2][3];
		
		for(int i=0;i<abc.length;i++)
		{
			for(int j=1;j<4;j++)
			{
				abc[i][j]=i*j;
				System.out.println(abc[i][j]);
			}
		}
		
		return abc;
	}
	
	
	
}
