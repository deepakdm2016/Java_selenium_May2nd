import org.openqa.selenium.By;
import org.openqa.selenium.JavascriptExecutor;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class disabledText {

	public static void main(String[] args) {

		System.setProperty("webdriver.chrome.driver", "C:\\Users\\Deepak\\Selenium\\Java_selenium_May2nd\\chromedriver.exe");
		WebDriver driver=new ChromeDriver();
		driver.get("https://www.facebook.com/");
		System.out.println(driver.findElement(By.xpath("//input[@id='u_0_s']")));
		
		JavascriptExecutor js=(JavascriptExecutor)driver;
		System.out.println(js.executeScript("document.getElementById('u_0_s').getAttribute('aria-label')").toString());
		js.executeScript("document.getElementById('u_0_s').value='DeepakDM'");
		System.out.println("Success");
		driver.close();
	}

}
