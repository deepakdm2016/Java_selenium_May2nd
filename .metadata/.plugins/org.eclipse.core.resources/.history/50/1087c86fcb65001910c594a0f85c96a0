import java.util.*;
public class Intersection_of_two_arrays {

	public static void main(String v[])
	{
		int a[]={1,2,3,4,5,7};
		int b[]={1,2,8,6};
		List<Integer> abc=new ArrayList<Integer>();
		
		for(int i=0;i<b.length;i++)
		{
			
			for(int j=1;j<a.length;j++)
			{
				if(a[i]==b[j] && (!abc.contains(a[i])))
				{
					abc.add(a[i])
				}
			}
		}
	
	}
}
