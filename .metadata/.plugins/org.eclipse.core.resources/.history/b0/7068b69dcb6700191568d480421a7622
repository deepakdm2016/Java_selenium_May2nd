package multithreading;

public class threaddemo extends Thread {
	
	public static void main() throws InterruptedException
	{
		
		Thread a=new threaddemo();
		a.start();
		
		for (int i=0;i<10;i++)
		{
			System.out.println(i);
		}
		
		
	}
	
	public void run()
	{
		for (int i=0;i<10;i++)
		{
			System.out.println(i);
			Thread.sleep(2000);
		}
		
	}
	
}
