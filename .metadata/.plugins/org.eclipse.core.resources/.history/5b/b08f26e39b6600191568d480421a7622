package test;

import org.testng.annotations.DataProvider;
import org.testng.annotations.Test;

public class basicClass2 {
	
	@Test(priority=1,groups={"regression"})
	public void method1()
	{
		System.out.println("God is great Savior of families in baseclass2");
	}

	
	@Test(priority=0,enabled=true,dataProvider="method3", groups={"regression", "smoke"})
	public void method2(Object[][] abcd)
	{
		System.out.println("method 2 - God is great Savior of families in baseclass2");
		
		for(int i=0;i<abcd.length;i++)
		{
			for(int j=0;j<3;j++)
			{
				System.out.println(abcd[i][j]);
			}
		}
		
	}

	@DataProvider
	public Object[][] method3()
	{
		
		Object[][] abc=new Object[2][3];
		
		for(int i=0;i<abc.length;i++)
		{
			for(int j=0;j<3;j++)
			{
				abc[i][j]=i*j;
			}
		}
		
		return abc;
	}
	
	
	
}
